#for循环结束后执行操作
idols=['nanamin','miona','nachan','maiyan']
for idol in idols:
    print(idol.title()+',ganbare!')
print('Arigatou,minna!')


numbers=list(range(1,5))
print(numbers)
#[1,2,3,4]
